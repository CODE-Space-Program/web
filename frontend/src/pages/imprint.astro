---
import * as post from "./imprint.md";

import Layout from "../layouts/Layout.astro";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
---

<Layout>
  <Header />
  <main
    style="max-width:48rem; padding: 6rem 2rem; margin: 0 auto; min-height: calc(100vh - 78px)"
  >
    <post.Content />
  </main>
  <Footer />
</Layout>
